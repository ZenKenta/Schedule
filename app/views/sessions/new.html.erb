<% @title ='ログイン' %>

<div id="login-form">
    <h1><%= @title %></h1>

    <%= form_for @form,url: :session do |f| %>
        <div>
        <%= f.label :account ,'アカウントID' %>
        <%= f.text_field :account %>
        </div>
        <div>
        <%= f.label :password,'パスワード' %>
        <%= f.password_field :password %>
        </div>

        <div>
        <%= f.submit 'ログイン' %>
        </div>
    <% end %>
</div>